    <Group name="atlas_impulse_response">
        <Attribute name="Description" type="String">
            <Value>Contains parameters to characterize the ATLAS pulse energy and pulse shape, derived from the Start Pulse Detector data. These parameters are at the ICESat-2 geolocation segment rate (~20m along-track)</Value>
        </Attribute>
        <Group name="pce1_spot1">
            <Attribute name="Description" type="String">
                <Value>Contains parameters to characterize the ATLAS impulse response from the TEP photon histograms available for two of the three strong beams.</Value>
            </Attribute>
            <Group name="tep_histogram">
                <Dimension name="tep_tod" size="1"/>
                <Dimension name="tep_hist_time" size="2000"/>
                <Int32 name="reference_tep_flag">
                    <Dim name="/atlas_impulse_response/pce1_spot1/tep_histogram/tep_tod"/>
                    <Attribute name="contentType" type="String">
                        <Value>auxiliaryInformation</Value>
                    </Attribute>
                    <Attribute name="description" type="String">
                        <Value>Flag that indicates the reference TEP has been used in place of a more recent TEP realization. 0=dynamic TEP used; 1=static reference TEP used.</Value>
                    </Attribute>
                    <Attribute name="flag_meanings" type="String">
                        <Value>dynamic_tep_used, reference_tep_used</Value>
                    </Attribute>
                    <Attribute name="flag_values" type="Int32">
                        <Value>0</Value>
                        <Value>1</Value>
                    </Attribute>
                    <Attribute name="long_name" type="String">
                        <Value>Reference TEP Used</Value>
                    </Attribute>
                    <Attribute name="source" type="String">
                        <Value>ATL03 ATBD, Section 7.2</Value>
                    </Attribute>
                    <Attribute name="units" type="String">
                        <Value>1</Value>
                    </Attribute>
                    <Attribute name="valid_max" type="Int32">
